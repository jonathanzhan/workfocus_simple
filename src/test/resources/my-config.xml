<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans   http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 配置自定义属性 -->
	<bean id="processEngineConfiguration" class="com.mfnets.workfocus.activiti.interceptor.MyProcessEngineConfiguration">
		<property name="jdbcUrl" value="jdbc:mysql://localhost:3306/workfocus" />
		<property name="jdbcDriver" value="com.mysql.jdbc.Driver" />
		<property name="jdbcUsername" value="root" />
		<property name="jdbcPassword" value="123456" />
		<property name="databaseSchemaUpdate" value="false"></property>
		<property name="userName" value="Jonathan"></property>
	</bean>

</beans>